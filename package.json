{
  "name": "adonis-ui-react-start",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "server": "node ace serve --watch",
    "client": "node ace mix:watch",
    "build": "yarn client:build && yarn server:build",
    "server:build": "node ace build --production",
    "client:build": "node ace mix:build --production",
    "dev": "concurrently \"yarn server\" \"yarn client\""
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.6.2",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@japa/preset-adonis": "^1.0.15",
    "@japa/runner": "^2.0.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "adonis-preset-ts": "^2.1.0",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.1.0",
    "laravel-mix": "^6.0.43",
    "pino-pretty": "^7.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.12.0",
    "react-scripts": "^5.0.1",
    "typescript": "~4.6",
    "web-vitals": "^2.1.4",
    "youch": "^3.1.1",
    "youch-terminal": "^2.1.3"
  },
  "dependencies": {
    "@adonisjs/core": "^5.7.5",
    "@adonisjs/repl": "^3.1.10",
    "@adonisjs/session": "^6.2.2",
    "@adonisjs/shield": "^7.0.10",
    "@adonisjs/view": "^6.1.3",
    "adonis-mix-asset": "^3.0.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
